        "axios": "^0.18",
        "bootstrap": "^4.1.3",
        "chart.js": "^2.7.2",
        "feather-icons": "^4.7.3",
        "jquery": "^3.3.1",
        "moment": "^2.22.2",
        "pickadate": "^3.5.6",
        "popper.js": "^1.14.4",
        "snackbarjs": "^1.1.0",
        "socket.io-client": "^2.1.1",
        "sweetalert": "^2.1.0",
